{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "src/**",
    "index.html"
  ],
  "builds": [
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}
